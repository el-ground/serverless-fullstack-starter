    "@tiptap/react": "^2.1.12",
    "@use-gesture/react": "^10.3.0",
    "bcrypt": "^5.1.1",
    "blueimp-load-image": "^5.16.0",
    "capacitor-native-settings": "^5.0.1",
    "capacitor-plugin-app-tracking-transparency": "^2.0.4",
    "capacitor-rate-app": "^4.0.3",
    "clamp": "^1.0.1",
    "cls-hooked": "^4.2.2",
