{
  "name": "node-service-starter",
  "version": "2.0.0",
  "description": "node backend service starter",
  "author": "<jwjang@elground.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "packageManager": "yarn@3.2.3",
  "engines": {
    "node": "20"
  },
  "type": "module",
  "scripts": {
    "set-config": "node ./scripts/set-config.js",
    "lint:js": "eslint --ignore-path .eslintignore \"+(src|server)/**/*.+(ts|tsx|js|jsx|cjs|mjs|json)\"",
    "lint:css": "stylelint --ignore-path .eslintignore \"src/**/*.+(css|sass|scss|less)\"",
    "prettier": "prettier --check --ignore-path .eslintignore \"+(src|server)/**/*.+(ts|tsx|js|jsx|cjs|mjs|json|css|sass|scss|less)\"",
    "typecheck": "tsc --noEmit",
    "lint": "yarn lint:js && yarn lint:css && yarn prettier && yarn typecheck",
    "clean": "rm -rf dist .next temp",
    "codegen": "graphql-codegen --config codegen.ts && node ./schema/combine.js",
    "copy:gql": "copyfiles schema/**/*.gql dist/",
    "package": "yarn set-config && yarn workspaces focus --all && yarn codegen && next build && yarn copy:gql && yarn build:sw && tsc --project ./tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "test": "node --experimental-test-coverage --test ./dist",
    "_dev:start": "tsc-alias -p tsconfig.server.json && yarn copy:gql && cross-env NODE_ENV=development node dist/server/index.js",
    "dev": "yarn clean && ENVIRONMENT=dev yarn set-config && (yarn codegen --watch src/**/*.(ts|tsx) & tsc-watch -p ./tsconfig.server.json --onCompilationStarted \"yarn codegen\" --onSuccess \"yarn _dev:start\")",
    "uglify:sw": "uglifyjs --compress --mangle --output ./public/sw.js -- ./public/sw.js",
    "build:sw": "tsc-bundle ./tsconfig.sw.json && yarn uglify:sw"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.1",
    "@capacitor/cli": "^5.5.1",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@svgr/webpack": "^8.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/clamp": "^1.0.3",
    "@types/cls-hooked": "^4.3.8",
    "@types/compression": "^1.7.5",
    "@types/cookie": "^0.5.4",
    "@types/cookie-parser": "^1.4.6",
    "@types/deep-equal": "^1.0.4",
    "@types/express": "^4.17.21",
    "@types/ip": "^1.1.3",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.10",
    "@types/node": "20.9.1",
    "@types/nprogress": "^0.2.3",
    "@types/pica": "^9.0.4",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/sharp": "^0.32.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "inkscape": "^3.1.1",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard-scss": "^11.1.0",
    "tsc-alias": "^1.8.8",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.2.2",
    "typescript-bundle": "^1.0.18",
    "typescript-plugin-css-modules": "^5.0.2",
    "uglify-js": "^3.17.4",
    "web-push": "^3.6.6"
  },
