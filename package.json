{
  "name": "node-service-starter",
  "version": "2.0.0",
  "description": "node backend service starter",
  "author": "<jwjang@elground.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "packageManager": "yarn@3.2.3",
  "engines": {
    "node": "20"
  },
  "type": "module",
  "scripts": {
    "lint:js": "eslint --ignore-path .eslintignore \"+(src|server)/**/*.+(ts|tsx|js|jsx|cjs|mjs|json)\"",
    "lint:css": "stylelint --ignore-path .eslintignore \"src/**/*.+(css|sass|scss|less)\"",
    "prettier": "prettier --check --ignore-path .eslintignore \"+(src|server)/**/*.+(ts|tsx|js|jsx|cjs|mjs|json|css|sass|scss|less)\"",
    "typecheck": "tsc --noEmit",
    "lint": "yarn lint:js && yarn lint:css && yarn prettier && yarn typecheck",
    "clean": "rm -rf dist .next",
    "codegen": "graphql-codegen --config codegen.ts",
    "copy:gql": "copyfiles schema/**/*.gql dist/",
    "package": "yarn workspaces focus --all && yarn codegen && next build && yarn copy:gql && tsc --project ./tsconfig.server.json && tsc-alias",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "test": "node --experimental-test-coverage --test ./dist",
    "_dev:start": "tsc-alias && yarn copy:gql && cross-env NODE_ENV=development node dist/server/index.js",
    "dev": "yarn clean && (yarn codegen --watch src/**/*.(ts|tsx) & tsc-watch -p ./tsconfig.server.json --onCompilationStarted \"yarn codegen\" --onSuccess \"yarn _dev:start\")"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/client-preset": "^4.0.1",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@types/cls-hooked": "^4.3.4",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.35",
    "@types/ip": "^1.1.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "18",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.44.0",
    "eslint-config-next": "^13.4.9",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "stylelint": "^15.10.1",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard-scss": "^10.0.0",
    "tsc-alias": "^1.8.6",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.6",
    "typescript-plugin-css-modules": "^5.0.1"
  },
  "dependencies": {
    "@apollo/client": "beta",
    "@apollo/experimental-nextjs-app-support": "^0.3.2",
    "@apollo/server": "^4.7.5",
    "@graphql-tools/schema": "^10.0.0",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dataloader": "^2.2.2",
    "express": "^4.18.2",
    "express-request-id": "^3.0.0",
    "glob": "^10.3.3",
    "graphql": "^16.7.1",
    "lru_map": "^0.4.1",
    "morgan": "^1.10.0",
    "next": "^13.4.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "supertest": "^6.3.3",
    "winston": "^3.9.0"
  }
}
